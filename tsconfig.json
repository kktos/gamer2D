{
  "include": ["src/**/*"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["esnext", "DOM"],
    "module":"ESNext",
    "moduleResolution": "Node",
    "declaration": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "isolatedDeclarations": false,
    "removeComments": true,
  }
}
